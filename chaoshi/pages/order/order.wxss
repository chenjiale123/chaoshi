.swiper-tab{
  width: 100%;
 height:86rpx;
background:rgba(255,255,255,1);
display: flex;
justify-content: space-around;
margin: 0 auto;
margin-top: 20rpx;
overflow: hidden

}
.swiper-tab-item{
  height: 60%;
font-size:26rpx;
font-family:PingFang SC;
font-weight:500;
color:rgba(102,102,102,1);
margin-top: 22rpx;
}
.active{
  border-bottom: 2px solid #F3B100;
  font-size:32rpx;
font-family:PingFang SC;
font-weight:bold;
color:rgba(51,51,51,1);
}


.main{
  width: 96%;
  height: auto;
  overflow: hidden;
  margin: 0 auto;
  position: relative;
}
.ul{
  width: 100%;
  background: #fff;
  height:auto;
background:rgba(255,255,255,1);
box-shadow:0px 0px 20rpx 0px rgba(0, 0, 0, 0.04);
border-radius:10rpx;
margin-top: 20rpx;
overflow: hidden;
padding-bottom: 30rpx;
}
.ul image{
  width: 238rpx;
  height: 158rpx;
  float: left;
  margin-top: 20rpx;
}
.ul .right{
  width: 300rpx;
  float: left;
  margin-left: 70rpx;
  height: 100%

}
.ul .right>text{
  display: block;
  font-size:26rpx;
font-family:PingFang SC;
font-weight:bold;
color:rgba(51,51,51,1);
margin-top: 15rpx;
}
.ul .right>.etc{
  color: #F93251;
}